{
  "name": "div2-item-compare-frontend",
  "version": "1.0.0",
  "description": "An item comparison site for the Division 2 game",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start -p 7777",
    "test": "NODE_ENV=test jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "plantdink",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "wesbos"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "@apollo/client": "3.5.7",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "apollo-upload-client": "^14.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.12.0",
    "chart.js": "^3.0.0",
    "date-fns": "^2.16.1",
    "downshift": "^6.0.6",
    "graphql": "16.0.1",
    "graphql-tag": "^2.11.0",
    "lodash.debounce": "^4.0.8",
    "next": "^10.0.3",
    "next-images": "^1.8.4",
    "next-with-apollo": "^5.1.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^17.0.1",
    "react-loader-spinner": "^6.0.0-0",
    "react-transition-group": "^4.4.1",
    "react-vivus": "^0.0.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.5.5",
    "vivus": "^0.4.6",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.3.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "casual": "^1.6.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.5.5"
  }
}
